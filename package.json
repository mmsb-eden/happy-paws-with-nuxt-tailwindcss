{
  "name": "happy-paws-with-nuxt-tailwindcss",
  "version": "0.0.0",
  "homepage": "https://happy-paws-with-nuxt-tailwindcss.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/happy-paws-with-nuxt-tailwindcss.git"
  },
  "scripts": {
    "dev": "nuxt dev",
    "now": "npm run build && npm run preview",
    "lighthouse:dev": "npx unlighthouse --no-cache --site http://localhost:3000",
    "lighthouse": "npx unlighthouse --no-cache --site https://happy-paws-with-nuxt-tailwindcss.netlify.app",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "analyze": "nuxt analyze",
    "vue-devtools": "vue-devtools",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts ./ --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts,html,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.1",
    "@vee-validate/nuxt": "^4.12.6",
    "@vueuse/core": "^10.9.0",
    "lite-youtube-embed": "^0.3.2",
    "pinia": "^2.1.7"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.194",
    "@nuxt/content": "^2.12.1",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/image": "^1.4.0",
    "@nuxtjs/critters": "^0.6.0",
    "@nuxtjs/fontaine": "^0.4.1",
    "@nuxtjs/html-validator": "^1.6.0",
    "@nuxtjs/seo": "^2.0.0-rc.9",
    "@pinegrow/nuxt-module": "^3.0.52",
    "@pinegrow/tailwindcss-plugin": "^3.0.20",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.11.30",
    "@unocss/nuxt": "^0.58.6",
    "@unocss/preset-icons": "^0.58.6",
    "@vue/devtools": "^7.0.19",
    "@vueuse/nuxt": "^10.9.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.11.1",
    "nuxt-icon": "^0.6.9",
    "postcss": "^8.4.37",
    "prettier": "^3.2.5",
    "rehype-external-links": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2"
  },
  "web-types": "web-types.json"
}
